{
  "name": "vlanet-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:websocket": "playwright test test/websocket/",
    "test:websocket:ui": "playwright test test/websocket/ --ui",
    "test:websocket:connection": "playwright test test/websocket/connection-stability.spec.ts",
    "test:websocket:collaboration": "playwright test test/websocket/multi-user-collaboration.spec.ts",
    "test:websocket:notifications": "playwright test test/websocket/real-time-notifications.spec.ts",
    "test:websocket:edge": "playwright test test/websocket/edge-conditions.spec.ts",
    "test:websocket:integration": "playwright test test/websocket/playwright-integration.spec.ts",
    "test:performance": "playwright test test/performance/",
    "test:performance:core-vitals": "playwright test test/performance/core-web-vitals.spec.ts",
    "test:performance:load": "playwright test test/performance/load-testing.spec.ts",
    "test:design": "playwright test --config=playwright-design-verification.config.ts",
    "test:design:tokens": "playwright test test/design-verification/design-tokens/ --config=playwright-design-verification.config.ts",
    "test:design:responsive": "playwright test test/design-verification/responsive-design/ --config=playwright-design-verification.config.ts",
    "test:design:accessibility": "playwright test test/design-verification/accessibility/ --config=playwright-design-verification.config.ts",
    "test:design:performance": "playwright test test/design-verification/performance-impact/ --config=playwright-design-verification.config.ts",
    "test:design:consistency": "playwright test test/design-verification/ui-consistency/ --config=playwright-design-verification.config.ts",
    "test:design:report": "playwright show-report test-results/design-verification-report",
    "test:load:k6": "k6 run test/performance/k6-load-test.js",
    "test:video-planning": "vitest run test/video-planning/",
    "test:video-planning:unit": "vitest run test/video-planning/unit/",
    "test:video-planning:integration": "vitest run test/video-planning/integration/",
    "test:video-planning:e2e": "playwright test test/video-planning/e2e/",
    "test:video-planning:all": "npm run test:video-planning:unit && npm run test:video-planning:integration",
    "test:load:autocannon": "autocannon -c 10 -d 60 http://localhost:3005",
    "test:load:heavy": "autocannon -c 50 -d 120 http://localhost:3005",
    "lighthouse:ci": "lhci autorun",
    "lighthouse:collect": "lhci collect --url=http://localhost:3005",
    "lighthouse:assert": "lhci assert",
    "performance:baseline": "npm run lighthouse:collect && npm run test:performance:core-vitals",
    "performance:monitor": "npm run lighthouse:ci && npm run test:performance",
    "clinic:doctor": "clinic doctor -- node server.js",
    "clinic:bubbleprof": "clinic bubbleprof -- node server.js",
    "clinic:flame": "clinic flame -- node server.js",
    "type-check": "tsc --noEmit",
    "test:ai-planning": "npm run test:ai-planning:unit && npm run test:ai-planning:integration && npm run test:ai-planning:e2e",
    "test:ai-planning:unit": "vitest test/ai-planning/unit/",
    "test:ai-planning:integration": "playwright test test/ai-planning/integration/",
    "test:ai-planning:e2e": "playwright test test/ai-planning/ai-planning-e2e.spec.ts",
    "test:ai-planning:quality": "node -e \"require('./test/ai-planning/quality-metrics.ts').DoQualityGate.runQualityGate(require('./test/ai-planning/quality-metrics.ts').SAMPLE_QUALITY_DATA)\"",
    "test:ai-planning:mock": "node test/ai-planning/mock-test-runner.js",
    "test:ai-planning:coverage": "vitest test/ai-planning/ --coverage",
    "ai-planning:dev": "npm run test:ai-planning:unit -- --watch",
    "ai-planning:ci": "npm run test:ai-planning && npm run test:ai-planning:quality",
    "test:api": "vitest test/api/",
    "test:api:unit": "vitest test/api/unit/",
    "test:api:integration": "vitest test/api/integration/",
    "test:api:performance": "vitest test/api/performance/",
    "test:api:ai": "vitest test/api/unit/ai-generate-plan.test.ts",
    "test:api:crud": "vitest test/api/unit/plans-crud.test.ts",
    "test:api:concurrent": "vitest test/api/performance/concurrency-load.test.ts",
    "test:api:e2e": "vitest test/api/integration/e2e-workflow.test.ts",
    "test:api:all": "tsx test/api/run-api-tests.ts",
    "test:api:watch": "vitest test/api/ --watch",
    "test:api:coverage": "vitest test/api/ --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.85.5",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.540.0",
    "moment": "^2.30.1",
    "next": "15.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^14.3.8",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.49.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autocannon": "^8.0.0",
    "autoprefixer": "^10.4.20",
    "clinic": "^13.0.0",
    "eslint": "9.16.0",
    "eslint-config-next": "15.1.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.1",
    "k6": "^0.0.0",
    "lighthouse": "^12.8.1",
    "msw": "^2.10.5",
    "postcss": "^8.4.47",
    "sass": "^1.82.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.2",
    "vitest": "^2.1.8",
    "web-vitals": "^5.1.0",
    "ws": "^8.18.0"
  }
}
