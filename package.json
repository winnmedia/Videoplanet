{
  "name": "vridge-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:arch": "eslint . --ext .ts,.tsx --config .eslintrc.fsd.js",
    "check:circular": "madge --circular --extensions ts,tsx ./src",
    "migrate:check": "node scripts/migration-check.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "cypress:component": "cypress open --component",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "e2e:open": "start-server-and-test dev http://localhost:3000 cypress",
    "test:all": "npm run test:ci && npm run e2e",
    "test:user-journey": "cypress run --spec 'cypress/e2e/user-journey/**/*'",
    "test:accessibility": "cypress run --spec 'cypress/e2e/accessibility/**/*'",
    "test:integration": "jest __tests__/integration/",
    "test:scenarios": "npm run test:user-journey && npm run test:accessibility && npm run test:integration",
    "validate-env": "node scripts/validate-env.js",
    "vercel-check": "npm run validate-env",
    "build:optimize": "node scripts/optimize-build.js",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "vercel:deploy-check": "npm run validate-env && npm run build:optimize"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/redux-persist": "^4.0.0",
    "antd": "^5.22.4",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "immer": "^10.1.1",
    "moment": "^2.30.1",
    "next": "^14.0.0",
    "query-string": "^8.1.0",
    "rc-util": "^5.44.4",
    "react": "^18.2.0",
    "react-datepicker": "^4.14.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.0",
    "react-switch": "^7.0.0",
    "react-toastify": "^11.0.5",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.62.1",
    "sharp": "^0.34.3",
    "styled-components": "^6.1.0",
    "swiper": "^10.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/redux-logger": "^3.0.13",
    "babel-jest": "^29.7.0",
    "cypress": "^13.6.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-plugin-boundaries": "^5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "madge": "^8.0.0",
    "postcss": "^8.4.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.0.0",
    "redux-logger": "^3.0.6",
    "start-server-and-test": "^2.0.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.0.0"
  }
}
